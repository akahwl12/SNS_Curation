ndomuser.me/api/portraits/men/73.jpg\"></v-img>\r\n        </v-list-item-avatar>\r\n      </v-list-item>\r\n\r\n      <div style=\"padding-top:10px; text-align:center\">\r\n        <p>{{nickName}}</p>\r\n        <p>{{email}}</p>\r\n      </div>\r\n\r\n      <div style=\"text-align:center; padding-top:20px\">\r\n        <div style=\"float:left; width:50%\">팔로워</div>\r\n        <div>팔로잉</div>\r\n      </div>\r\n\r\n      <div style=\"text-align:center; padding-top:10px; padding-bottom:20px\">\r\n        <div style=\"float:left; width:50%\">{{this.follower}}</div>\r\n        <div>{{this.following}}</div>\r\n      </div>\r\n\r\n      <v-divider style=\"margin-left:10%; width:80%\"></v-divider>\r\n\r\n      <v-list dense>\r\n        <div style=\"padding-top:5%; padding-bottom:5%; padding-left:10%\" @click=\"modifyProfile\">\r\n          회원정보 수정\r\n        </div>\r\n        <div style=\"padding-top:5%; padding-bottom:5%; padding-left:10%\" @click=\"findPassword\">\r\n          비밀번호 설정\r\n        </div>\r\n      </v-list>\r\n\r\n      <v-divider style=\"margin-left:10%; width:80%\"></v-divider>\r\n\r\n      \r\n        <div style=\"padding-top:5%; padding-bottom:5%; padding-left:10%\" @click=\"Address\">\r\n          뉴스피드 설정\r\n        </div>\r\n     \r\n\r\n      <v-divider style=\"margin-left:10%; width:80%\"></v-divider>\r\n\r\n      <v-list dense>\r\n        <div style=\"padding-top:5%; padding-bottom:5%; padding-left:10%\">\r\n          통계\r\n        </div>\r\n      </v-list>\r\n\r\n      <v-divider style=\"margin-left:10%; width:80%\"></v-divider>\r\n      <v-list dense>\r\n        <div style=\"padding-top:5%; padding-bottom:5%; padding-left:10%\" @click=\"$store.dispatch('logout')\">\r\n          로그아웃\r\n        </div>\r\n      </v-list>\r\n\r\n    </v-navigation-drawer>\r\n</template>\r\n\r\n<script>\r\nimport {mapState} from 'vuex';\r\nimport Axios from \"axios\";\r\n  export default {\r\n    props : ['drawer'],\r\n    data () {\r\n      return {\r\n        propDrawer : this.drawer,\r\n        nickName : '',\r\n        email : '',\r\n        follower : 5,\r\n        following : 5,\r\n      }\r\n    },\r\n    created() {\r\n      if(this.$store.state.userinfo!=null) {\r\n          this.email = this.$store.state.userinfo.email;\r\n      }\r\n      this.propDrawer = this.drawer;\r\n\r\n      this.getUserInfo();\r\n    },\r\n    watch : {\r\n      propDrawer : function(v) {\r\n        this.updateParentNavi();\r\n      }\r\n    },\r\n    methods : {\r\n      getUserInfo() {\r\n          let form = new FormData()\r\n            form.append('email', this.email)\r\n            // console.log(data.email);\r\n            Axios.post(\"http://192.168.31.103:8080/user/getUserInfo\", form)\r\n            .then(Response => {\r\n              this.nickName = Response.data.nickname;\r\n              this.email = Response.data.email;\r\n            })\r\n            .catch(Error => {\r\n            })\r\n      },\r\n      updateParentNavi() {\r\n        this.$emit('child', this.propDrawer)\r\n      },  \r\n      findPassword() {\r\n        var router = this.$router;\r\n          router.push({\r\n              name: \"ModifyPasswordSendEmail\"\r\n          });\r\n      },\r\n      modifyProfile() {\r\n        this.$router.push({\r\n          name:\"ModifyProfile\"\r\n        })\r\n      },\r\n      Address() {\r\n\r\n      },\r\n    },\r\n    computed : {\r\n      ...mapState(['userinfo']),\r\n    }\r\n  }\r\n</script>"]}]}