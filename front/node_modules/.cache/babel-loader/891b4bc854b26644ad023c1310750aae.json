AA,KAAA,CAAA,KAAA,EAAA,IAAA,IAAA,CAAA,EAAA;AACA,YAAA,KAAA,GAAA,IAAA,QAAA,EAAA;;AACA,YAAA,IAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,QAAA,CAAA,QAAA;;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,YAAA,EAAA,IAAA;;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,UAAA,EAAA,KAAA,KAAA,CAAA,KAAA,EAAA,QAAA;;AAEA,QAAA,IAAA,CAAA,IAAA,CAAA,mBAAA,EAAA,KAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA;;AACA,UAAA,MAAA,CAAA,qBAAA,CAAA,MAAA,CAAA,KAAA,CAAA,KAAA,EAAA,KAAA,EAAA,KAAA;;AACA,cAAA,QAAA,CAAA,IAAA,KAAA,SAAA,EAAA;AACA,YAAA,KAAA,CAAA,eAAA,CAAA;AACA,WAFA,MAGA,IAAA,QAAA,CAAA,IAAA,KAAA,QAAA,EAAA;AACA,YAAA,KAAA,CAAA,mBAAA,CAAA;AACA;AACA,SAVA,EAWA,KAXA,CAWA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,SAbA;AAeA;AACA,KA5FA;AA6FA,IAAA,UA7FA,sBA6FA,KA7FA,EA6FA;AAAA;;AACA,UAAA,IAAA,GAAA,IAAA,QAAA,EAAA;AACA,UAAA,GAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,QAAA,CAAA,QAAA;AACA,MAAA,IAAA,CAAA,MAAA,CAAA,YAAA,EAAA,GAAA;AACA,MAAA,IAAA,CAAA,MAAA,CAAA,UAAA,EAAA,KAAA,KAAA,CAAA,KAAA,EAAA,QAAA;AACA,MAAA,IAAA,CAAA,IAAA,CAAA,kBAAA,EAAA,IAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,MAAA,CAAA,QAAA,EAAA,KAAA,EAAA,CAAA;;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,QAAA,EAFA,CAGA;AACA,OALA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OARA;AASA;AA3GA,GAPA;AAqHA,EAAA,IAAA,EAAA;AAAA,WAAA;AACA,MAAA,KAAA,EAAA,EADA;AAEA,MAAA,KAAA,EAAA,EAFA;AAGA,MAAA,IAAA,EAAA,CAHA;AAIA,MAAA,GAAA,EAAA,CAJA;AAKA,MAAA,QAAA,EAAA,EALA;AAMA,MAAA,KAAA,EAAA,EANA;AAOA,MAAA,MAAA,EAAA,EAPA;AAQA,MAAA,QAAA,EAAA,EARA;AASA,MAAA,UAAA,EAAA,EATA;AAUA,MAAA,cAAA,EAAA;AAVA,KAAA;AAAA;AArHA,CAAA","sourcesContent":["<template>\r\n<v-app>\r\n    <div class=\"user\" id=\"follow\">\r\n        <div class=\"header\" style=\"width:100%; height:auto\">\r\n            <div style=\"width:35px; float:left;\">\r\n            <button v-on:click=\"goNewsFeeds\">\r\n                <img src=\"../../assets/images/backIcon.png\" style=\"width:100%;\">\r\n            </button>\r\n            </div>\r\n            <p style=\"vertical-align: middle;padding: 3px 5px;float:left; font-size:25px\">팔로워 리스트</p>\r\n        </div>\r\n        <v-card\r\n    max-width=\"500\"\r\n    class=\"mx-auto\"\r\n  >\r\n    \r\n\r\n    <v-list subheader style=\"margin-top:50px\">\r\n      <v-subheader>팔로워 리스트</v-subheader>\r\n\r\n      <v-list-item\r\n        v-for=\"(item, index) in items\"\r\n        :key=\"item.title\"\r\n      >\r\n\r\n        <v-list-item-content>\r\n          {{item.nickname}}\r\n          <v-list-item-subtitle v-text=\"item.email\"></v-list-item-subtitle>\r\n        </v-list-item-content>\r\n\r\n        <div class=\"profile-card-ctr\" v-if=\"isfollow[index]==0\">\r\n            <button class=\"profile-card__button button--orange\" @click=\"followgo(index)\">Follow</button>\r\n        </div>\r\n        <div class=\"profile-card-ctr\" v-if=\"isfollow[index]==1\">\r\n            <button class=\"profile-card__button button--gray\" @click=\"unfollowgo(index)\">UnFollow</button>\r\n        </div>\r\n      </v-list-item>\r\n    </v-list>\r\n\r\n    <v-divider></v-divider>\r\n\r\n    <v-list subheader>\r\n      <v-subheader>아직 수락하지 않은 사람들</v-subheader>\r\n\r\n      <v-list-item\r\n        v-for=\"item in items2\"\r\n        :key=\"item.title\"\r\n      >\r\n        <v-list-item-avatar>\r\n          <v-img :src=\"item.avatar\"></v-img>\r\n        </v-list-item-avatar>\r\n\r\n        <v-list-item-content>\r\n          닉네임:<v-list-item-title v-text=\"item.title\"></v-list-item-title>\r\n        </v-list-item-content>\r\n\r\n        <v-list-item-content>\r\n          이메일:<v-list-item-subtitle v-text=\"item.subtitle\"></v-list-item-subtitle>\r\n        </v-list-item-content>\r\n      </v-list-item>\r\n    </v-list>\r\n  </v-card>\r\n    </div>\r\n</v-app>\r\n\r\n</template>\r\n\r\n\r\n\r\n\r\n<script>\r\n\r\n    import '../../assets/css/style.scss'\r\n    import '../../assets/css/user.scss'\r\n    import UserApi from '../../apis/UserApi'\r\n    import http from '../../../http-common'\r\n    import {fireDB} from '../../main'\r\nimport { firestore } from 'firebase'\r\n\r\n\r\n    export default {\r\n        components: {},\r\n        created() {\r\n          this.nickname = this.$store.state.userinfo.nickName;\r\n          this.getUserByNickname(this.nickname);\r\n        },\r\n        watch: {},\r\n        methods: {\r\n            setAlarm(alarm) {\r\n                this.alarmCount.push(alarm);\r\n            },\r\n            updateAlarmToFirebase(email) {\r\n                fireDB.collection('Alarm').doc(email)\r\n                .update({\r\n                    count : firestore.FieldValue.increment(1)\r\n                })\r\n            },\r\n            watchAlarmFromFirebase(email) {\r\n                let whoami = this;\r\n                let count=0;\r\n                fireDB.collection('Alarm').doc(email).onSnapshot( {\r\n                    includeMetadataChanges: true    \r\n                },function(doc) {\r\n                    if(doc.data()==undefined) {\r\n                        count = 0;\r\n                    } else {\r\n                        count = doc.data().count;\r\n                    }\r\n                    whoami.setAlarm(count);\r\n                })\r\n            },\r\n            goNewsFeeds() {\r\n                var router = this.$router;\r\n                router.push({\r\n                    name: \"MainPage\"\r\n                });\r\n            },\r\n            getFollower(num){\r\n              http.get(\"follow/getFollower/{num}?num=\"+ num)\r\n              .then(Response => {\r\n                // console.log(Response)\r\n                this.items = Response.data.object;\r\n                this.isfollow = Response.data.object2;\r\n                console.log(this.isfollow)\r\n\r\n              })\r\n            },\r\n            getUserByNickname(nick) {\r\n                let form = new FormData()\r\n                form.append('nickname', nick)\r\n                http.get(\"/user/userinfo/{nickname}?nickname=\" + nick)\r\n                .then(Response => {\r\n                  this.num = Response.data.num;\r\n                    this.email = Response.data.email;\r\n                    this.getFollower(this.num);\r\n                    this.watchAlarmFromFirebase(this.email);\r\n                })\r\n                .catch(Error => {\r\n                    console.log(Error)\r\n                })\r\n            },\r\n            followgo(index){\r\n                if(this.items[index].auth==0) {\r\n                    let form = new FormData();\r\n                    let myn  = this.$store.state.userinfo.nickName;\r\n                    form.append('mynickname', myn)\r\n                    form.append('nickname',this.items[index].nickname)\r\n                    http.post(\"/follow/follow\", form)\r\n                    .then(Response => {\r\n                        this.$set(this.isfollow,index,1)\r\n                        this.updateAlarmToFirebase(this.items[index].email);\r\n                        // console.log(Response.data)\r\n                    })\r\n                    .catch(Error => {\r\n                        console.log(Error)\r\n                    })\r\n                }\r\n                else if(this.items[index].auth==1) {\r\n                    let form = new FormData();\r\n                    let myn  = this.$store.state.userinfo.nickName;\r\n                    form.append('mynickname', myn)\r\n                    form.append('nickname',this.items[index].nickname)\r\n                    \r\n                    http.post(\"/follow/nonfollow\", form)\r\n                    .then(Response => {\r\n                        console.log(Response)\r\n                        this.updateAlarmToFirebase(this.items[index].email, index);\r\n                        if(Response.data==='success') {\r\n                            alert(\"팔로우가 요청되었습니다.\")\r\n                        }\r\n                        else if(Response.data==='failed') {\r\n                            alert(\"이미 팔로우 신청을 하였습니다.\")\r\n                        }\r\n                    })\r\n                    .catch(Error => {\r\n                        console.log(Error)\r\n                    })\r\n\r\n                }\r\n            },\r\n            unfollowgo(index){\r\n                let form = new FormData()\r\n                let myn  = this.$store.state.userinfo.nickName;\r\n                form.append('mynickname', myn)\r\n                form.append('nickname',this.items[index].nickname)\r\n                http.post(\"/follow/unFollow\", form)\r\n                .then(Response => {\r\n                  this.$set(this.isfollow,index,0)\r\n                    console.log(this.isfollow)\r\n                    // console.log(Response.data)\r\n                })\r\n                .catch(Error => {\r\n                    console.log(Error)\r\n                })\r\n            },\r\n            \r\n        },\r\n    data: () => ({\r\n      email:'',\r\n      intro:'',\r\n      auth:0,\r\n      num:0,\r\n      nickname:'',\r\n      items: [],\r\n      items2: [],\r\n      isfollow:[],\r\n      alarmCount:[],\r\n      userAlarmCount : 0,\r\n    }),\r\n    }\r\n</script>"],"sourceRoot":"src/views/user"}]}